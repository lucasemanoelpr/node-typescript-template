{
  "name": "great-capital-core",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "lint:check": "eslint -c .eslintrc --ext .ts src",
    "start": "DEBUG=api-ts-template* node dist/index.js",
    "dev": "set DEBUG=api-ts-template* && nodemon src/index.ts",
    "test": "NODE_ENV=test jest \"^.+test\\.ts$\" --runInBand --detectOpenHandles",
    "windows:test": "set NODE_ENV=test jest \"^.+test\\.ts$\" --runInBand --detectOpenHandles",
    "test:watch": "jest --watch \"^.+test\\.ts$\"",
    "load:test": "cd load-test && ./run.sh",
    "pact:publish": "npm run build && node dist/infrastructure/pact/publisher/Publisher.js",
    "pact:verify": "jest \"^.+pactverify\\.ts$\"",
    "pact:test": "jest \"^.+pacttest\\.ts$\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d src/infrastructure/database/mysql/AppDataSource.ts",
    "migrate": "typeorm-ts-node-commonjs migration:run -d src/infrastructure/database/mysql/AppDataSource.ts",
    "revert": "typeorm-ts-node-commonjs migration:revert -d src/infrastructure/database/mysql/AppDataSource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/md5": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "26.1.0",
    "jest-pact": "^0.9.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "tsyringe": "^4.7.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.44",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "googleapis": "^122.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "marked": "^6.0.0",
